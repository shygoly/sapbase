{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "lines": 70,
  "functions": 70,
  "branches": 70,
  "statements": 70,
  "reporter": ["text", "lcov", "html", "json-summary"],
  "exclude": [
    "**/*.spec.ts",
    "**/*.e2e-spec.ts",
    "**/node_modules/**",
    "**/dist/**",
    "**/test/**",
    "**/main.ts",
    "**/*.module.ts",
    "**/*.config.ts",
    "**/migrations/**",
    "**/seeds/**"
  ]
}
